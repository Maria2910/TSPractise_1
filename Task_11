declare module 'str-utils' {

    function strReverse(value: string): string;

    function strToLower(value: string): string;

    function strToUpper(value: string): string;

    function strRandomize(value: string): string;

    function strInvertCase(value: string): string;

    export {strReverse, strToLower, strToUpper, strRandomize, strInvertCase};
}
